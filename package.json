    "gatsby-plugin-google-analytics": "2.1.23",
    "gatsby-plugin-manifest": "2.2.26",
    "gatsby-plugin-offline": "3.0.17",
    "gatsby-plugin-react-helmet": "3.1.13",
    "gatsby-plugin-react-svg": "2.1.2",
    "gatsby-plugin-robots-txt": "1.5.0",
    "gatsby-plugin-sass": "2.1.20",
    "gatsby-plugin-sharp": "2.2.36",
    "gatsby-plugin-sitemap": "2.2.19",
    "gatsby-plugin-typography": "2.3.15",
    "gatsby-plugin-use-dark-mode": "1.1.2",
    "gatsby-remark-autolink-headers": "2.1.16",
    "gatsby-remark-external-links": "0.0.4",
    "gatsby-remark-images": "3.1.28",
    "gatsby-remark-prismjs": "3.3.20",
    "gatsby-source-filesystem": "2.1.35",
    "gatsby-transformer-remark": "2.6.32",
    "gatsby-transformer-sharp": "2.3.2",
    "node-sass": "4.13.0",
    "prismjs": "1.17.1",
    "prop-types": "15.7.2",
    "react": "16.10.2",
    "react-dom": "16.10.2",
    "react-helmet": "5.2.1",
    "react-icons": "3.8.0",
    "react-typography": "0.16.19",
    "typography": "0.16.19"
  },
  "devDependencies": {
    "husky": "3.0.9",
    "lint-staged": "9.4.2",
    "prettier": "1.18.2"
  },
  "keywords": [
    "karlo delalic",
    "gatsby",
    "full stack developer",
    "personal website"
  ],
  "license": "MIT",
  "scripts": {
    "build": "./build.sh && gatsby build",
    "dev": "./build.sh && gatsby develop",
    "format": "prettier --write src/**/*.{js,jsx,scss,md}",
    "serve": "gatsby serve",
    "test": "echo \"Write tests! -> https://gatsby.dev/unit-testing\""
  },
  "lint-staged": {
    "src/**/*.{js,jsx,scss}": [
      "prettier --write",
      "git add"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/kdelalic/karlodelalic.com-gatsby"
  },
  "bugs": {
    "url": "https://github.com/kdelalic/karlodelalic.com-gatsby/issues"
  }
}
