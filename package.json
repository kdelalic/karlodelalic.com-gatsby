  "keywords": [
    "karlo delalic",
    "gatsby",
    "software engineer",
    "personal website"
  ],
  "license": "MIT",
  "scripts": {
    "build": "./build.sh && gatsby build",
    "dev": "./build.sh && gatsby develop",
