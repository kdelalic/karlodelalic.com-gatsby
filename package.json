{
  "name": "karlodelalic.me",
  "private": true,
  "description": "A personal portfolio website.",
  "version": "0.1.0",
  "author": "Karlo Delalic",
  "dependencies": {
    "@material-ui/core": "4.9.7",
    "gatsby": "2.20.2",
    "gatsby-image": "2.3.0",
    "gatsby-plugin-google-analytics": "2.2.0",
    "gatsby-plugin-manifest": "2.3.1",
    "gatsby-plugin-offline": "3.1.0",
    "gatsby-plugin-react-helmet": "3.2.0",
    "gatsby-plugin-react-svg": "3.0.0",
    "gatsby-plugin-robots-txt": "1.5.0",
    "gatsby-plugin-sass": "2.2.0",
    "gatsby-plugin-sharp": "2.5.1",
    "gatsby-plugin-sitemap": "2.3.0",
    "gatsby-plugin-typography": "2.4.0",
    "gatsby-plugin-use-dark-mode": "1.1.2",
    "gatsby-remark-autolink-headers": "2.2.0",
    "gatsby-remark-external-links": "0.0.4",
    "gatsby-remark-images": "3.2.0",
    "gatsby-remark-prismjs": "3.4.0",
    "gatsby-source-filesystem": "2.2.1",
    "gatsby-transformer-remark": "2.7.0",
    "gatsby-transformer-sharp": "2.4.1",
    "node-sass": "4.13.1",
    "prismjs": "1.19.0",
    "prop-types": "15.7.2",
    "react": "16.13.1",
    "react-dom": "16.13.1",
    "react-helmet": "5.2.1",
    "react-icons": "3.9.0",
    "react-typography": "0.16.19",
    "typography": "0.16.19"
  },
  "devDependencies": {
    "husky": "4.2.3",
    "lint-staged": "10.0.9",
    "prettier": "1.18.2"
  },
  "keywords": [
    "karlo delalic",
    "gatsby",
    "full stack developer",
    "personal website"
  ],
  "license": "MIT",
  "scripts": {
    "build": "./build.sh && gatsby build",
    "dev": "./build.sh && gatsby develop",
    "format": "prettier --write src/**/*.{js,jsx,scss,md}",
    "serve": "gatsby serve",
    "test": "echo \"Write tests! -> https://gatsby.dev/unit-testing\""
  },
  "lint-staged": {
    "src/**/*.{js,jsx,scss}": [
      "prettier --write",
      "git add"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/kdelalic/karlodelalic.com-gatsby"
  },
  "bugs": {
    "url": "https://github.com/kdelalic/karlodelalic.com-gatsby/issues"
  }
}
