{
  "name": "karlodelalic.me",
  "private": true,
  "description": "A personal portfolio website.",
  "version": "0.1.0",
  "author": "Karlo Delalic",
  "dependencies": {
    "@material-ui/core": "4.4.2",
    "gatsby": "2.15.16",
    "gatsby-image": "2.2.19",
    "gatsby-plugin-google-analytics": "2.1.16",
    "gatsby-plugin-manifest": "2.2.16",
    "gatsby-plugin-offline": "3.0.7",
    "gatsby-plugin-react-helmet": "3.1.7",
    "gatsby-plugin-react-svg": "2.1.2",
    "gatsby-plugin-sass": "2.1.14",
    "gatsby-plugin-sharp": "2.2.22",
    "gatsby-plugin-sitemap": "2.2.12",
    "gatsby-plugin-typography": "2.3.7",
    "gatsby-remark-autolink-headers": "2.1.10",
    "gatsby-remark-external-links": "0.0.4",
    "gatsby-remark-prismjs": "3.3.13",
    "gatsby-source-filesystem": "2.1.22",
    "gatsby-transformer-remark": "2.6.22",
    "gatsby-transformer-sharp": "2.2.14",
    "node-sass": "4.12.0",
    "prismjs": "1.17.1",
    "prop-types": "15.7.2",
    "react": "16.10.2",
    "react-dom": "16.10.2",
    "react-helmet": "5.2.1",
    "react-icons": "3.7.0",
    "react-typography": "0.16.19",
    "typography": "0.16.19"
  },
  "devDependencies": {
    "husky": "3.0.5",
    "lint-staged": "9.2.5",
    "prettier": "1.18.2"
  },
  "keywords": [
    "karlo delalic",
    "gatsby",
    "full stack developer",
    "personal website"
  ],
  "license": "MIT",
  "scripts": {
    "build": "./build.sh && gatsby build",
    "dev": "./build.sh && gatsby develop",
    "format": "prettier --write src/**/*.{js,jsx,scss,md}",
    "serve": "gatsby serve",
    "test": "echo \"Write tests! -> https://gatsby.dev/unit-testing\""
  },
  "lint-staged": {
    "linters": {
      "src/**/*.{js,jsx,scss}": ["prettier --write", "git add"]
    }
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/kdelalic/karlodelalic.com-gatsby"
  },
  "bugs": {
    "url": "https://github.com/kdelalic/karlodelalic.com-gatsby/issues"
  }
}
