  "devDependencies": {
    "husky": "4.2.5",
    "lint-staged": "10.5.3",
    "prettier": "1.18.2"
  },
  "keywords": [
    "karlo delalic",
    "gatsby",
    "full stack developer",
    "personal website"
  ],
  "license": "MIT",
  "scripts": {
    "build": "./build.sh && gatsby build",
    "dev": "./build.sh && gatsby develop",
    "format": "prettier --write src/**/*.{js,jsx,scss,md}",
    "serve": "gatsby serve",
    "test": "echo \"Write tests! -> https://gatsby.dev/unit-testing\""
  },
  "lint-staged": {
    "src/**/*.{js,jsx,scss}": [
      "prettier --write",
      "git add"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/kdelalic/karlodelalic.com-gatsby"
  },
  "bugs": {
    "url": "https://github.com/kdelalic/karlodelalic.com-gatsby/issues"
